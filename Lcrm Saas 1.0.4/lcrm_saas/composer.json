{
    "name": "lorvent/lcrm_saas",
    "description": "Next generation CRM web application.",
    "keywords": ["framework", "laravel", "crm"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "barryvdh/laravel-dompdf": "^0.8.0",
        "cartalyst/sentinel": "^2.0",
        "chaibi/gtranslator": "^1.1",
        "dingo/api": "1.0.x@dev",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~3.3",
        "laracasts/flash": "^3.0",
        "laravel-notification-channels/pusher-push-notifications": "^1.0",
        "laravel/cashier": "^7.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^2.1",
        "maddhatter/laravel-fullcalendar": "^1.3",
        "mpociot/vat-calculator": "^2.2",
        "omnipay/paypal": "~2.6.3",
        "paypal/adaptivepayments-sdk-php": "3.9.2",
        "prettus/l5-repository": "^2.6",
        "pusher/pusher-php-server": "2.6.*",
        "spatie/flysystem-dropbox": "^1.0",
        "spatie/laravel-backup": "^5.2",
        "srmklive/paypal": "^1.6",
        "tymon/jwt-auth": "^1.0@dev",
        "venturecraft/revisionable": "^1.28",
        "yajra/laravel-datatables-oracle": "^8.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1.0",
        "barryvdh/laravel-ide-helper": "^2.4.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^2.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
